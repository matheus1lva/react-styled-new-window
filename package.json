{
  "name": "react-new-window-styles",
  "version": "1.3.1",
  "description": "A react portal which creates a new window and supports styled-components",
  "homepage": "https://github.com/PlayMa256/react-styled-new-window",
  "bugs": "https://github.com/PlayMa256/react-styled-new-window/issues",
  "repository": {
