    "url": "git+https://github.com/PlayMa256/react-styled-new-window"
  },
  "keywords": [
    "styled-components",
    "styled",
    "components",
    "style",
    "window",
    "react",
    "portal"
  ],
  "author": "Matheus Silva <matheus.g.silva@hotmail.com>",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --hot --config ./example/webpack.config.js",
