    "rimraf": "^2.7.1",
    "sass-loader": "^8.0.2",
    "styled-components": "^5.1.0",
    "styles-loader": "^2.0.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
